{
  "expo": {
    "name": "MagicFingers",
    "slug": "MagicFingers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/MagicFingers_Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Loading_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#C5FFFF",
      "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "The app accesses your photos to let you share them with your friends."
          }
        ]
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundImage": "./assets/MagicFingers_Logo.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
